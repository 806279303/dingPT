/* components/commodity/good_add/good_add.wxss */
.add_good_container{
    width: 100%;
    height: 90vh;
    background-color: white;
    position: fixed;
    z-index: 99;
    top:0;
    display: flex;  
    flex-direction: column;
    box-sizing: border-box;
    padding-top: 100rpx;
}
.add_good_container .backToCommdity{
    width: 150rpx;
    height: 55rpx;
    position: absolute;
    top:20rpx;
    left:30rpx;
}
.add_good_container .backToCommdity button{
    width: 100%;
    height: 100%;
    line-height: 55rpx;
    font-size: 30rpx;
}
.add_good_list{
    width: 100%;
    display: flex;  
    flex-flow: row nowrap;
    justify-content: center;
    align-items: center;
    /* border-bottom:2rpx solid saddlebrown; */
    font-size: 30rpx;
    margin-bottom: 30rpx;
    position: relative;
    /* background-color: #00cc99; */
}
.add_good_container .de_info{
    margin-bottom: 25rpx;
    padding-bottom: 25rpx;
    border-bottom: 2rpx solid rgba(0, 0, 0, .8);
}
.add_good_list .add_list_row{
    display: flex;
    flex-direction: column;
    width: 100%;
    justify-content: center;
    border-bottom: 2rpx solid rgba(0, 0, 0, .8);
    /* background-color: blueviolet; */
}
.add_good_list .add_list_row .add_list_halfrow{
    display: flex;
    justify-content: center;
    margin-bottom: 20rpx;
    /* background-color: chocolate; */
}
.add_good_list .del_list{
    width: 30rpx;
    height: 30rxp;
    border:3rpx solid #313131;
    background-color: rgba(255, 255, 255, 1);
    box-shadow: 5rpx 5rpx 5rpx rgba(0, 0, 0, .1);
    color: #313131;
    border-radius: 50%;
    position: absolute;
    right:3rpx;
    top:-30rpx;
    font-weight: bold;
    text-align: center;
    font-size:20rpx;
    line-height: 30rpx;
}

.add_good_list .list_add_btn button{
    width: 130rpx;
    height: 55rpx;
    line-height: 55rpx;
    font-size: 30rpx;
    color: white;
    border-radius: 10rpx;
    /* background-color: #00cc99; */
    margin-left: 40rpx;
}
.add_good_list .del_btn button{
    margin-left: 5rpx;
}
.add_good_list .list_input{
    /* width: 220rpx; */
    height: 50rpx;
    position: relative;
}
.add_good_list .list_input input{
    border: 1rpx solid #313133;

}
.add_good_list .list_input  .list_input_txt{
    width: 160rpx;
    height: 50rpx;
    border:2rpx solid #313131;
    box-sizing: border-box;
    font-size: 28rpx;
    overflow: hidden;
}
.add_good_list .list_input .list_option{
    position: absolute;
    width: 100%;
    display: flex;
    flex-direction: column;
    text-align: center;
    top:50rpx;
    background-color: white;
    z-index: 99;
}
.add_good_list .list_input .list_option .list_row{
    width: 100%;
    height: 45rpx;
    line-height: 45rpx;
    box-sizing: border-box;
    border-width: 0 2rpx 2rpx 2rpx;
    border-color: #313131;
    border-style: solid;
    color: #313131;
    background-color: white;
    text-align: left;
    font-size: 25rpx;
}
.list_detail{
    display: flex;  
    flex-flow: row nowrap;
    /* justify-content: space-around; */
    align-items: center;
}
.list_detail .type input{
    width: 130rpx;
}
.list_detail .good_price input{
    width: 95rpx;
    padding-left: 5rpx;
}
.list_detail .power{
    display: flex;
}
.list_detail .power button{
    width: 145rpx;
    font-size: 23rpx;
    height: 50rpx;
    line-height: 50rpx;
}
.list_right{
    margin-left: 10rpx;
}
.add_btn{
    width: 400rpx;
    height: 50rpx;
    line-height: 50rpx;
    font-size: 40rpx;
    font-weight: bold;
    border:2rpx solid #313131;
    border-radius: 30rpx;
    margin:0 auto;
    margin-top: 20rpx;
    margin-bottom: 30rpx;
}
/* .list_txt{
    font-weight: bold;
} */
.add_sub button{
    width: 230rpx;
    height: 55rpx;
    line-height: 55rpx;
    font-size: 36rpx;
    color: white;
    border-radius: 20rpx;
    /* background-color: #00cc99; */
    margin-top: 50rpx;
}


.stockBox{
    position: fixed;
    width: 300rpx;
    top: 25vh;
    left: 50%;
    margin-left: -150rpx;
    border:2rpx solid #313131;
    border-radius: 15rpx;
    overflow: hidden;
    background-color: aliceblue;
}
.stockBox .header{
    width: 100%;
    line-height: 40rpx;
    border-bottom: 2rpx solid #313131;
    text-align: right;
}
.stockBox .header .close{
    margin-right: 10rpx;
    font-size:28rpx;
}
.stockBox .writeStockPsw{
    width: 100%;
    padding: 20rpx 0;
}
.stockBox .writeStockPsw input{
    width:70%;
    height: 27rpx;
    font-size: 20rpx;
    border: 2rpx solid #313131;
    margin:0 auto;
    padding:0 5rpx;
}
.stockBox button{
    width: 100%;
    height: 45rpx;
    color:white;
    line-height: 40rpx;
    font-size: 25rpx;
    border-radius: 0;
}

.add_box{
    width: 300rpx;
    padding: 20rpx 0 20rpx 0;
    display: flex;
    justify-content: center;
    text-align: center; 
    flex-direction: column;
    background-color: white;
    box-shadow: 10rpx 10rpx 20rpx rgba(0, 0, 0, .2);
    position: absolute;
    left:50%;
    margin-left: -150rpx;
    top: 25%;
    z-index: 99;
}
.add_box .title{
    font-size: 30rpx;
    font-weight: bold;
}
.add_box .add_input {
    margin-top: 20rpx;
    width: 100%;
    display: flex;
    justify-content: center;
}
.add_box .add_input input{
    border:2rpx solid #313131;
    width: 80%;
    height: 30rpx;
    text-align: left;
}
.add_box .sub_btn{
    width: 100%;
    margin-top: 20rpx;
    display: flex;
    justify-content: center;
}
.add_box .sub_btn button{
    width: 80%;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 25rpx;
}
.add_box .closeAdd{
    position: absolute;
    top:10rpx;
    right:20rpx;
    font-size: 28rpx;
}

.powerClientInfo{
    width: 100%;
    position: fixed;
    top: 10%;
    box-shadow: 10rpx 0rpx 30rpx rgba(0, 0, 0, .1);
    display: flex;
    flex-direction: column;
    justify-content: center;
    background-color: white;    
    border: 2rpx solid #e0e0e0;
    z-index: 1001;
}
.powerClientInfo .closePowerClientInfo{
    width: 30rpx;
    height: 30rpx;
    font-size: 35rpx;
    position: absolute;
    top:10rpx;
    right:10rpx;
}
.powerClientInfo .title{
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    height: 80rpx;
    font-size: 40rpx;
    border-bottom: 2rpx solid #313131;
}
.powerClientInfo .search{
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    height: 70rpx;
}
.powerClientInfo .search input{
    border:1rpx solid #313131;
    width: 80%;
    height: 50rpx;
    padding-left: 5rpx;
}
.powerClientInfo .clientTb{
    width: 100%;
    display: flex;
    flex-direction: column;
}
.powerClientInfo .clientTb .row{
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
}
.powerClientInfo .clientTb .row .col{
    border:1rpx solid #313131;
    box-sizing: border-box;
    font-size: 28rpx;
    display: flex;
    justify-content:center;
    align-items: center;
    height: 60rpx;
    overflow: hidden;
}
.powerClientInfo .clientTb .row .col1{
    width: 21%;
}
.powerClientInfo .clientTb .row .col2{
    width: 20%;
}
.powerClientInfo .clientTb .row .col3{
    width: 26%;
}
.powerClientInfo .clientTb .row .col4{
    width: 24%;
}
.powerClientInfo .clientTb .row .col5{
    width: 9%;
}
.powerClientInfo .clientTb .row .col .noselect{
    width: 30rpx;
    height: 30rpx;
    border: 1rpx solid #313131;
    border-radius: 50%;
}
.powerClientInfo .clientTb .row .col .selected{
    width: 30rpx;
    height: 30rpx;
    /* border: 1rpx solid #313131; */
    border-radius: 50%;
    background-color: #313131;
}
.powerClientInfo .clientTb .row .bold{
    font-weight: bold;
}
.powerClientInfo .subPowerClient{
    width: 100%;
    display: flex;
    justify-content: center;
    margin-top: 30rpx;
}
.powerClientInfo .subPowerClient button{
    width: 80%;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 30rpx;
}

.modelContainer{
    width: 600rpx;
    left:50%;
    margin-left: -300rpx;
    border: 2rpx solid rgba(0, 0, 0, .2);
    position: fixed;
}
.modelContainer .modelBox{
    width: 80%;
    position: relative;
    margin:20rpx auto 0;
}
.modelContainer .modelBox .modelTxt{
    width: 100%;
    height: 50rpx;
    font-size: 28rpx;
    border:2rpx solid #313131;
    padding-left: 2rpx;
    line-height: 50rpx;
    box-sizing: border-box;
    font-weight: bold;
}
.modelContainer .modelBox .modelList{
    position: absolute;
    width: 100%;
    top:50rpx;
    z-index: 999;
    background-color: white;
}
.modelContainer .modelBox .modelList .modelRow {
    width: 100%;
    height: 50rpx;
    font-size: 28rpx;
    border-left:2rpx solid #313131;
    border-right:2rpx solid #313131;
    border-bottom:2rpx solid #313131;
    padding-left: 2rpx;
    line-height: 50rpx;
    box-sizing: border-box;
    display: flex;
    padding:3rpx 10rpx;
    position: relative;
}
.modelContainer .modelBox .modelList .modelRow .good_kind{
    width: 50%;
    text-align: center;
}
.modelContainer .modelBox .modelList .modelRow .good_name{
    width: 50%;
    text-align: center;
}
.modelContainer .modelBox .modelList .modelRow .delCurModel{
    position: absolute;
    width: 30rpx;
    height: 30rpx;
    border: 2rpx solid rgba(0, 0, 0, .8);
    border-radius: 50%;
    right:10rpx;
    top:10rpx;
    line-height: 26rpx;
}
.modelContainer .addModel{
    width: 70%;
    margin:30rpx auto 0;
}
.modelContainer .subModel{
    margin:10rpx auto 0;
}
.modelContainer .addModel button{
    width: 100%;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 29rpx;
}
